{
  "name": "backend-express",
  "version": "1.0.0",
  "description": "node express apollo graphql # environment node 20.17.0  npm 10.8.2",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --watch /src/index.ts  --trace-warnings ",
    "build": "tsc -watch",
    "start": "node dist/index.js",
    "start2": "npm run compile && node ./dist/index.js",
    "typeorm": "typeorm-ts-node-esm",
    "migrations-create": "typeorm-ts-node-esm migration:create ./src/migrations/migration",
    "migrations-generate": "typeorm-ts-node-esm migration:generate ./src/migrations/migration -d ./src/data-source.ts",
    "migrations-run": "typeorm-ts-node-esm migration:run -d ./src/data-source.ts",
    "migrations-revert": "typeorm-ts-node-esm migration:revert -d ./src/data-source.ts",
    "migrations-run-fake": "typeorm-ts-node-esm migration:run -d ./src/data-source.ts --fake",
    "migrations-revert-fake": "typeorm-ts-node-esm migration:revert -d ./src/data-source.ts --fake",
    "migrations-show": "typeorm-ts-node-esm migration:show -d ./src/data-source.ts ",
    "lint": "eslint . --fix",
    "test": "jest --coverage --detectOpenHandles --forceExit --runInBand"
  },
  "keywords": [],
  "author": "",
  "dependencies": {
    "@apollo/server": "^4.11.0",
    "@graphql-tools/schema": "^10.0.6",
    "@graphql-tools/utils": "^10.5.4",
    "@types/es6-shim": "^0.31.45",
    "body-parser": "^1.20.3",
    "common-js": "^0.3.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.20.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.9.0",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/graphql": "^14.5.0",
    "@types/node": "^22.5.4",
    "eslint": "^8.58.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.2.2",
    "jest": "^30.5.0",
    "nodemon": "^3.1.4",
    "prettier": "^3.0.1",
    "ts-jest": "^31.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "packageManager": "yarn@3.6.1+sha512.de524adec81a6c3d7a26d936d439d2832e351cdfc5728f9d91f3fc85dd20b04391c038e9b4ecab11cae2b0dd9f0d55fd355af766bc5c1a7f8d25d96bb2a0b2ca"
}
